export default class OrientationService {  constructor (game) {    this.game = game;    this.checkOrientation();  }  checkOrientation () {    this.game.scale.forceOrientation(true, false);    if (this.game.scale.isPortrait) {      OrientationService.orientationOverlay('turn', 'block');    }    this.game.scale.enterIncorrectOrientation.add(() => {      OrientationService.orientationOverlay('turn', 'block');    });    this.game.scale.leaveIncorrectOrientation.add(() => {      OrientationService.orientationOverlay('turn', 'none');    });  }  static orientationOverlay (div, display) {    document.getElementById(div).style.display = display;  }}